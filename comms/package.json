{
  "author": "korlend",
  "name": "extorris-comms",
  "version": "1.0.0",
  "description": "Service for communications with user",
  "main": "index.js",
  "license": "ISC",
  "type": "module",
  "scripts": {
    "clean": "rimraf ./dist/* --glob",
    "build": "tsc && tsc-alias",
    "start": "node ./dist/index.js",
    "start:inspect": "node --inspect ./dist/index.js",
    "clean-build": "npm-run-all -n -l clean build",
    "cbs": "npm-run-all -n -l clean build start",
    "cbsi": "npm-run-all -n -l clean build start:inspect",
    "ds": "npm-run-all -n -l build start",
    "dsi": "npm-run-all -n -l build start:inspect",
    "dev": "tsc-watch --onSuccess \"npm run cbs\"",
    "devi": "tsc-watch --onSuccess \"npm run cbsi\""
  },
  "devDependencies": {
    "@tsconfig/node21": "^21.0.3",
    "@types/amqplib": "^0",
    "@types/cors": "^2",
    "@types/express": "^4.17.21",
    "@types/node": "^22.4.1",
    "@types/ws": "^8.5.14",
    "body-parser": "^1.20.3",
    "module-alias": "^2.2.3",
    "npm-run-all": "^4.1.5",
    "prettier": "3.3.3",
    "redis": "^4.7.0",
    "rimraf": "^6.0.1",
    "tsc-alias": "^1.8.10",
    "tsc-watch": "^6.2.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.5",
    "typescript-transform-paths": "^3.5.1"
  },
  "dependencies": {
    "amqplib": "^0.10.5",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^5.0.0",
    "extorris-common": "portal:../extorris-common",
    "ws": "^8.18.0"
  },
  "packageManager": "yarn@4.5.0"
}
