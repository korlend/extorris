{
  "author": "korlend",
  "name": "savage-grove-backend",
  "version": "1.0.0",
  "description": "Backend core library of atsequencer",
  "main": "index.js",
  "license": "ISC",
  "type": "module",
  "scripts": {
    "clean": "rimraf ./dist/* --glob",
    "build": "tsc && tsc-alias",
    "start": "node ./dist/index.js",
    "start:inspect": "node --inspect ./dist/index.js",
    "clean-build": "npm-run-all -n -l clean build",
    "cbs": "npm-run-all -n -l clean build start",
    "cbsi": "npm-run-all -n -l clean build start:inspect",
    "ds": "npm-run-all -n -l build start",
    "dsi": "npm-run-all -n -l build start:inspect",
    "dev": "tsc-watch --onSuccess \"npm run cbsi\""
  },
  "devDependencies": {
    "@tsconfig/node21": "^21.0.3",
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2",
    "@types/express": "^4.17.21",
    "@types/mysql": "^2.15.26",
    "@types/node": "^22.4.1",
    "@types/uuid": "^10.0.0",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.3",
    "change-case": "^5.4.4",
    "http-errors": "^2.0.0",
    "module-alias": "^2.2.3",
    "mysql": "^2.18.1",
    "mysql2": "^3.11.3",
    "npm-run-all": "^4.1.5",
    "prettier": "3.3.3",
    "reflect-metadata": "^0.2.2",
    "rimraf": "^6.0.1",
    "simplex-noise": "^4.0.3",
    "tsc-alias": "^1.8.10",
    "tsc-watch": "^6.2.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.5",
    "typescript-transform-paths": "^3.5.1",
    "uuid": "^10.0.0"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^5.0.0"
  },
  "packageManager": "yarn@4.5.0"
}
